{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "ReactApp",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "react",
    "lib": ["es6", "dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "es5"
  }
}
